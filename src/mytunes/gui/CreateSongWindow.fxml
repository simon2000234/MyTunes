<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mytunes.gui.CreateSongWindowController">
   <children>
      <Label fx:id="lblWindowTitle" layoutX="158.0" layoutY="14.0" text="New/Edit Song">
